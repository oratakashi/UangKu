<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Button">
        <attr name="text" format="string" />
        <attr name="type">
            <enum name="enabled" value="0" />
            <enum name="disabled" value="1" />
            <enum name="loading" value="2" />
        </attr>
        <attr name="button_background">
            <enum name="primaryBlue" value="0" />
            <enum name="red" value="1" />
        </attr>
        <attr name="allCaps" format="boolean" />
    </declare-styleable>
</resources>